<!DOCTYPE html>
<html>
  <head>
    <title>TheRialNews</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= javascript_include_tag "application", "data-turbo-track": "reload", defer: true %>
    
    
  

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

  <!-- Vendor CSS Files -->
  <%= stylesheet_link_tag "bootstrap.min" %>
  <%= stylesheet_link_tag "bootstrap-icons" %>
  <%= stylesheet_link_tag "boxicons.min" %>
  <%= stylesheet_link_tag "glightbox.min" %>
  <%= stylesheet_link_tag "remixicon" %>
  <%= stylesheet_link_tag "swiper-bundle.min" %>

  <!-- Template Main CSS File -->
  <%= stylesheet_link_tag "style" %>

  </head>
  <body>
    <header id="header" class="fixed-top ">
      <div class="container d-flex align-items-center justify-content-between">
        <%= render 'shared/navbar' %>
      </div>
    </header><!-- End Header -->

    <!-- ======= Hero Section ======= -->
  
  
  
    
  <main id="main">  
    <%= yield %>
  </main>
    
    <%= render 'footer/footer' %>

  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

  <!-- Vendor JS Files -->
  <%= javascript_include_tag "bootstrap.bundle.min" %>
  <%= javascript_include_tag "glightbox.min" %>
  <%= javascript_include_tag "isotope.pkgd.min" %>
  <%= javascript_include_tag "swiper-bundle.min" %>
  <%= javascript_include_tag "validate" %>

  <!-- Template Main JS File -->
  <%= javascript_include_tag "main" %>

  </body>
</html>
